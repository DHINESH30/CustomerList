<button class="closeButton" {{action "closeCus" }}>Close</button>
<button class="addButton" {{action "transAddUserRoute" }}>+New</button>
<table>
    <tr>
        <th> NAME</th>
        <th>USER ID</th>
        <th> EMAIL</th>
    </tr>
    {{#each-in this.model as |key curCus|}}
    <tr class="tableRow">
        <td {{action "customerDetail" key}}>{{curCus.Name}}</td>
        <td {{action "customerDetail" key}}>{{curCus.userId}}</td>
        <td {{action "customerDetail" key}}>{{curCus.emailId}}</td>
    </tr>
    {{/each-in}}
</table>

{{outlet}}